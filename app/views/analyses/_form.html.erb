 <%= form_for @analysis, html: { multipart: true } do |f| %>
  <% if @analysis.errors.any? %>
    <div id="errors">
      <h2><%= pluralize(@analysis.errors.count, "error") %> prevented this analysis from saving:</h2>
      <ul>
        <% @analysis.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>


  <%= f.label :title %><br>
  <%= f.text_field :title %>
  
  <br>
  <br>
  
  <%= f.label :subtitle %><br>
  <%= f.text_field :subtitle %>
  
  <br>
  <br>

  <%= f.label :content %><br>
  <%= f.text_area :content %>
  
  <br>
  <br>

  <%= f.submit "Publish" %>
<% end %>